{
	"manifest_version": 3,
	"name": "NipahTV Dev",
	"description": "A Better Kick and 7TV Emote Integration for Kick.",
	"version": "1.3.6",
	"content_scripts": [
		{
			"matches": ["https://kick.com/*"],
			"js": [
				"dist/vendor/jquery.min.js",
				"dist/vendor/dexie.min.js",
				"dist/vendor/fuse.min.js",
				"dist/vendor/twemoji.min.js",
				"dist/debug.user.js"
			],
			"css": ["dist/css/kick.css", "dist/css/kick.css.map"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": ["assets/*.png", "assets/btn/*.png"],
			"matches": ["https://kick.com/*"]
		}
	]
}
